body, html {
  height: 100%;
  margin: 0;
  font: 400 15px/1.8 "Lato", sans-serif;
  color: #777;
}

.bgimg-1, .bgimg-2, .bgimg-3 {
  position: relative;
  opacity: 0.65;
  background-attachment: fixed;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;

}
.bgimg-1 {
  background-image: url("assets/intro_10.png");
  min-height: 100%;
}

.bgimg-2 {
  background-image: url("img_parallax2.jpg");
  min-height: 400px;
}

.bgimg-3 {
  background-image: url("assets/");
  min-height: 400px;
}

.caption {
  position: absolute;
  left: 0;
  top: 50%;
  width: 100%;
  text-align: center;
  color: #000;
}

.caption span.border {
  background-color: #520001;
  color: #fff;
  padding: 18px;
  font-size: 25px;
  letter-spacing: 10px;
}

/*Navbar*/
.navbar { 
	position: absolute;
    left: 0px;
    height: 40px;
    
    width: 100%;
    display: flex;
    justify-content: center;
}
/*#logo a { color: #fff; text-decoration: none; float: left; font-size: 30px; margin-top: 20px; color: #fff; font-family:"Wellfleet"; font-weight: bold; }*/
.navbar ul{
	list-style: none;
	display: inline-block;
	margin: 0 auto;
	list-style: none;
}
.navbar a {
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.navbar li{
	margin-top: 9px;
	float: left;
	padding-left: 21px;
}
.navbar li a { 
	color: #fff; 
	opacity:0.6; 
	font-size: 16px; 
	text-decoration: none; 
	font-family: "Lato", sans-serif /*'Wellfleet'*/
	font-weight: bold; 
}
.navbar li a.active {
	color: #fff; 
	opacity:1; 
	border-bottom: 2px solid #fff; 
}
.navbar li a:hover { 
	color: #fff; 
	opacity:1; 
}
h3 {
  letter-spacing: 5px;
  text-transform: uppercase;
  font: 20px "Lato", sans-serif;
  color: #111;
}
p {
	text-align: center;
  color: #47090A;
}
.carousel-caption h5 {
  font: 20px "Lato", sans-serif;
  color: whitesmoke;
}
.carousel-caption p {
  font: 14px "Lato", sans-serif;
  color: whitesmoke;
}
/*Show me everything button*/
