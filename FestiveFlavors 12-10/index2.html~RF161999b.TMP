  float: right;
}
.show-me-everything a {
  text-decoration: none;
  font: 1em "Lato", sans-serif;
  text-transform: uppercase;
  display: inline-block;
  margin-top: 20px;
  padding: 10px 10px 10px 10px;/*8px 16px;*/
}
.show-me-everything a:hover {
  background-color: #ddd;
  text-decoration: none;
  color: black;
}
.show-me-everything .next {
  background-color: #47090A;
  color: white;
}
.show-me-everything .round {
